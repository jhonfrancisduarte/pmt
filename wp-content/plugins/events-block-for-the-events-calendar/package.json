{
  "name": "ebec-guten-block",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js"
  },
  "dependencies": {
    "@scarf/scarf": "^1.0.6",
    "@wordpress/components": "^16.0.0",
    "@wordpress/compose": "^5.0.1",
    "@wordpress/server-side-render": "^3.0.1",
    "autoprefixer": "^7.2.4",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-preset-cgb": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "cgb-dev-utils": "^1.6.0",
    "chalk": "^2.3.0",
    "classnames": "^2.3.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.1.0",
    "dompurify": "^2.2.6",
    "eslint": "^4.15.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-wordpress": "^0.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "filesize": "^3.5.11",
    "fs-extra": "^5.0.0",
    "gzip-size": "^4.1.0",
    "inquirer": "^5.0.0",
    "jquery": "^3.6.0",
    "next-google-fonts": "^2.2.0",
    "ngx-slick-carousel": "^0.5.1",
    "node-sass": "^4.7.2",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.10",
    "raw-loader": "^0.5.1",
    "react-slick": "^0.28.1",
    "resolve-pkg": "^1.0.0",
    "sanitize-html-react": "^1.13.0",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.0",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.23.1",
    "uikit-react": "^3.1.2",
    "update-notifier": "^2.3.0",
    "webpack": "^3.1.0"
  },
  "browser": {
    "crypto": false,
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "stream": false,
    "tls": false
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 Chrome versions",
              "last 2 Firefox versions",
              "last 2 Safari versions",
              "last 2 iOS versions",
              "last 1 Android version",
              "last 1 ChromeAndroid version",
              "ie 11"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread"
      ],
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      [
        "transform-react-jsx",
        {
          "pragma": "wp.element.createElement"
        }
      ],
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "devDependencies": {
    "classnames-loader": "^2.1.0"
  }
}
